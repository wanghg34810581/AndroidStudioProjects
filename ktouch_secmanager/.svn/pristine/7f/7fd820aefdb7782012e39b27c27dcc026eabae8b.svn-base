<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:secmanager="http://schemas.android.com/apk/res-auto"
    android:id="@+id/checkup_layout_t"
    android:orientation="vertical"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/round_progress_layout_t"
        android:layout_width="match_parent"
        android:layout_height="351dp"
        android:layout_alignParentTop="true"
        android:background="@color/checkupgreen"
        >
        <com.guli.secmanager.widget.RoundProgressBarWidthNumber
            android:id="@+id/round_progress_bar_t"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:progress="100"
            android:layout_centerInParent="true"
            secmanager:progress_unreached_bar_height="9dp"
            secmanager:progress_unreached_color="#ff6bc545"
            secmanager:progress_reached_color="#ffd3eec8"
            secmanager:progress_text_color="#ffc51B03"
            secmanager:progress_text_size="20sp"
            secmanager:radius="101dp"
            secmanager:radius_color="#ffd3eec8"
            secmanager:radius_background_color="#ff6bc545"
            secmanager:radius_height="9dp"
            secmanager:outer_circle_radius="108dp"
            secmanager:outer_circle_radius_color="#ffa1da88"
            secmanager:outer_circle_height="1dp"
            secmanager:shadow_radius="98dp"
            secmanager:shadow_color="#ff389a0e"
            secmanager:shadow_height="1dp"/>
        <TextView
            android:id="@+id/tv_score_value_t"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textSize="70sp"
            android:textColor="@color/checkupSubInfo"
            android:background="#00000000"
            />
        <TextView
            android:id="@+id/tv_score_unit_t"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_toRightOf="@id/tv_score_value_t"
            android:paddingLeft="5dp"
            android:paddingTop="40dp"
            android:textSize="15sp"
            android:text="@string/unit"
            android:textColor="@color/checkupMainInfo"
            android:background="#00000000"
            />
    </RelativeLayout>
    <TextView
        android:id="@+id/tv_curtain"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/round_progress_layout_t"
        android:background="@color/checkupgreen"/>

</RelativeLayout>